{
  "name": "hockeystick-image-convert-api",
  "version": "1.0.0",
  "description": "Image Convert API Microservice",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon dist/index.js",
    "local": "cross-env NODE_ENV=local nodemon dist/index.js",
    "build": "npm run build-ts",
    "rebuild": "rm -rf node_modules/ && rm -rf dist/ && rm -rf package-lock.json && rm -rf logs && rm -rf coverage && npm i && npm run build",
    "watch": "npm run watch-ts",
    "build:clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "npm run lint -- --fix",
    "build-ts": "ttsc",
    "watch-ts": "ttsc -w",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "npm run test -- --watch",
    "docker:compose:build": "docker-compose build",
    "docker:compose:access": "docker-compose -f docker-compose.yml -f docker-compose.access.yml up",
    "docker:build:dev": "docker build . --tag gcr.io/nemo-dev-266318/hockeystick-image-convert-api",
    "docker:run:dev": "docker run -p 9090:80 -e PORT=80 -e K_SERVICE=dev -e K_CONFIGURATION=dev -e K_REVISION=dev-00001 -e GOOGLE_APPLICATION_CREDENTIALS=/tmp/keys/nemo-dev.json -v %GOOGLE_APPLICATION_CREDENTIALS%:/tmp/keys/nemo-dev.json:ro gcr.io/nemo-dev-266318/hockeystick-image-convert-api"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "node": {
    "__dirname": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/vishvajit79/hockeystick-image-convert-api.git"
  },
  "author": "Vishvajit Kher",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/bluebird-global": "^3.5.12",
    "@types/cls-hooked": "^4.3.0",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "^24.9.1",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.5.0",
    "@types/request-promise": "^4.1.46",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.1",
    "@types/uuid": "^3.4.6",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.0.0",
    "ts-nameof": "^4.2.2",
    "ts-node": "^8.6.2",
    "ttypescript": "^1.5.10",
    "typemoq": "^2.1.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^3.0.5",
    "agentkeepalive": "^4.1.0",
    "bluebird": "^3.7.2",
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.8.0",
    "class-validator": "^0.11.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "extend": "^3.0.2",
    "helmet": "^3.21.2",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemon": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "uuid": "^3.4.0",
    "winston": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run build"
    }
  }
}
